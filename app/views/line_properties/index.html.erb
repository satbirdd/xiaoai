<h1>Listing line_properties</h1>

<table>
  <thead>
    <tr>
      <th>Targetable</th>
      <th>Targetable type</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @line_properties.each do |line_property| %>
      <tr>
        <td><%= line_property.targetable_id %></td>
        <td><%= line_property.targetable_type %></td>
        <td><%= line_property.name %></td>
        <td><%= link_to 'Show', line_property %></td>
        <td><%= link_to 'Edit', edit_line_property_path(line_property) %></td>
        <td><%= link_to 'Destroy', line_property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Line property', new_category_line_property_path %>
